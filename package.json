{
    "name": "ulid-workers",
    "version": "1.1.0",
    "description": "ULID generator for Cloudflare workers",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "npm run clean && tsc",
        "clean": "rimraf dist",
        "format": "prettier --write \"{source,test}/**/*.{js,ts}\"",
        "prepublishOnly": "npm run build",
        "test": "npm run build && npm run test:specs && npm run test:format",
        "test:format": "prettier --check \"{source,test}/**/*.{js,ts}\"",
        "test:specs": "nyc mocha -t 10000 'test/**/*.spec.js'",
        "prepare": "husky install"
    },
    "files": [
        "dist/**/*"
    ],
    "lint-staged": {
        "{source,test}/**/*.{js,ts}": [
            "prettier --write"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ryan-mars/ulid-workers.git"
    },
    "keywords": [
        "ulid",
        "uuid",
        "id",
        "generator",
        "guid"
    ],
    "author": "Ryan Marsh <ryan@stochastic.dev>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ryan-mars/ulid-workers/issues"
    },
    "homepage": "https://github.com/ryan-mars/ulid-workers#readme",
    "devDependencies": {
        "@types/node": "^17.0.15",
        "chai": "^4.3.6",
        "husky": "^7.0.4",
        "lint-staged": "^12.3.3",
        "mocha": "^9.2.0",
        "nyc": "^15.1.0",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "sinon": "^13.0.1",
        "typescript": "^4.5.5"
    },
    "dependencies": {}
}
